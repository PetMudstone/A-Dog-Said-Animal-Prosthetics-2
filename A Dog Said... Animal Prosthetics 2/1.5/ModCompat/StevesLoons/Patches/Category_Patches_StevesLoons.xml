<?xml version="1.0" encoding="utf-8"?>

<!-- patches common loon from Steve's Loons as category 2-->
<Patch>
	<Operation Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[
					@Name="Category2" or
					@Name="Category1"
				]/recipeUsers</xpath>
				<value>
					<li>SZ_CommonLoon</li>
				</value>
	</Operation>
</Patch>